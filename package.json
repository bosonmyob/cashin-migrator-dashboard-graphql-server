{
  "name": "cashin-migrator-dashboard-graphql-server",
  "version": "0.1.0",
  "description": "a graphql server for cashin migrator dashboard",
  "main": "server.js",
  "scripts": {
    "build": "npm install && npm run lint && npm run test",
    "coverage": "jest --coverage --verbose --silent",
