    "aws-sdk": "^2.991.0",
    "eslint": "^7.32.0",
    "semver": "^7.3.5"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  }
}
